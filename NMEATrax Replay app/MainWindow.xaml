<Window x:Class="NMEATrax_Replay_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NMEATrax_Replay_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="openBtn" Content="Open" HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top" Cursor="Hand" Click="Button_Click"/>
        <TextBox x:Name="outputBox" HorizontalAlignment="Left" Margin="92,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="473" Focusable="False" IsHitTestVisible="False" IsTabStop="False" Height="283"/>
        <TextBox x:Name="rpmBox" HorizontalAlignment="Left" Margin="589,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" AllowDrop="False" Focusable="False" IsHitTestVisible="False"/>
    </Grid>
</Window>
